{
    "fileExtensions": ["less"],
    "attributeQuotes": {
        "enabled": true
    },
    "borderZero": {
        "enabled": true
    },
    "comment": {
        "enabled": true
    },
    "decimalZero": {
        "enabled": true,
        "style": "none"
    },
    "depthLevel": {
        "enabled": false,
        "depth": 5
    },
    "duplicateProperty": {
        "enabled": true,
        "exclude": ["background-color"]
    },
    "finalNewline": {
        "enabled": true
    },
    "hexLength": {
        "enabled": true,
        "style": "short"
    },
    "importantRule": {
        "enabled": false
    },
    "qualifyingElement": {
        "allowWithAttribute": true,
        "allowWithClass": true
    },
    "selectorNaming": {
        "enabled": true
    },
    "spaceAfterPropertyColon": {
        "enabled": true,
        "style": "one_space"
    },
    "stringQuotes": {
        "enabled": true,
        "style": "double"
    },
    "propertyOrdering": {
        "enabled": false
    },
    "zeroUnit": {
        "style": "no_unit"
    }
}
